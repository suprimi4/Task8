<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="541.0" prefWidth="672.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.task8.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <HBox prefHeight="420.0" prefWidth="778.0">
            <children>
                <Pane fx:id="paneLight" prefHeight="390.0" prefWidth="308.0" />
                <VBox alignment="TOP_CENTER" prefHeight="390.0" prefWidth="392.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#addLights" prefHeight="25.0" prefWidth="160.0" text="Гирлянда" />
                        <Button mnemonicParsing="false" onAction="#drawStar" prefHeight="25.0" prefWidth="163.0" text="Звезда">
                            <VBox.margin>
                                <Insets top="50.0" />
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#addPresents" prefHeight="25.0" prefWidth="165.0" text="Подарки">
                            <VBox.margin>
                                <Insets top="50.0" />
                            </VBox.margin>
                        </Button>
                  <Button mnemonicParsing="false" onAction="#butZvPr" prefHeight="25.0" prefWidth="166.0" text="Звезда + подарки">
                     <VBox.margin>
                        <Insets top="40.0" />
                     </VBox.margin>
                  </Button>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                    <padding>
                        <Insets top="50.0" />
                    </padding>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="122.0" prefWidth="778.0" spacing="50.0">
            <children>
                <Button mnemonicParsing="false" onAction="#addAll" prefHeight="25.0" prefWidth="158.0" text="Добавить все" />
                <Button mnemonicParsing="false" onAction="#deleteAll" prefHeight="25.0" prefWidth="161.0" text="Удалить все" />
            </children>
        </HBox>
    </children>
</VBox>
